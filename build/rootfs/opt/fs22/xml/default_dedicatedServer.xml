    <game description="Farming Simulator 22" name="FarmingSimulator2022" exe="FarmingSimulator2022Game.exe">
        <imprint active="false"><![CDATA[
            <b>Sample Imprint Title</b><br>
            <br>
            Sample Imprint Text2<br>
            <br>
            Sample Imprint Text3<br>
        ]]></imprint>
        <logos>
            <login>template/loginLogo.jpg</login>
            <bottom>template/bottomLogo.jpg</bottom>
        </logos>
    </game>
</server>
